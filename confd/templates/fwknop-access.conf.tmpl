# Generated by confd {{datetime}}
{{range gets "/fwknop/users/*"}}
{{$user := json .Value}}
{{$username := base .Key}}
# <{{$username}}>
SOURCE                             ANY
REQUIRE_USERNAME                   {{$username}}
OPEN_PORTS                         {{$user.access}}
GPG_DECRYPT_ID                     {{getv "/fwknop/gpgDecryptId"}}
GPG_ALLOW_NO_PW                    Y
GPG_REMOTE_ID                      {{$user.gpgKeyId}}
GPG_REQUIRE_SIG                    Y
GPG_IGNORE_SIG_VERIFY_ERROR        N
HMAC_KEY_BASE64                    {{$user.hmacKeyBase64}}
# </{{$username}}>
{{end}}
